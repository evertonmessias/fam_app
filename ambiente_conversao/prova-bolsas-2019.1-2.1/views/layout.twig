	<style>
		#resultado {
			background-color: #ec008c!important;
			cursor: pointer;
		}

		#resultado:hover {
			background-color: #ff0097!important;
		}
		#modal-resultado {
			display: none;
			position: fixed;
			top: 0;
			left: 0;
			width: 100vw;
			height: 100vh;
			background: rgba(0,0,0,.7);
			z-index: 99999999;
			text-align: center;
		}
		#modal-resultado button#close {
			position: absolute;
			top: 10px;
			right: 30px;
			background: red;
			transition: .2s ease;
			color: #fff;
			padding: .5em 1em;
			font-weight: bold;
			border: 0;
			border-radius: 2px;
			cursor: pointer;
			z-index: 999999999;
		}

		#modal-resultado button#close:hover {
			background: darkred;
		}

		.resultados a {
			display: inline-block;
			padding: 1em 1.4em;
			border-radius: 0.2em;
			background-color: #052b62;
			color: #fff;
			transition: all 0.2s ease;
			min-width: 150px;
			margin-top: 50vh;
			text-decoration: none;
		}
		.resultados a:hover {
			background-color: #ec008c;
    		color: #fff;
		}



	</style>

